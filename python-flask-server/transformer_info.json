{
    "name": "MSigDB hypergeometric enrichment filter",
    "label": "MSigDB filter",
    "version": "1.3.0",
    "function": "filter",
    "description": "Gene-list filter based on hypergeometric enrichment in MSigDB gene sets (http://software.broadinstitute.org/gsea/index.jsp).",
    "properties": {
        "list_predicate": "related_to",
        "member_predicate": "related_to",
        "source_url": "http://software.broadinstitute.org/gsea/index.jsp",
        "method": "enrichment"
    },
    "parameters": [
        {
            "default": "1e-05",
            "name": "maximum p-value",
            "type": "double",
            "allowed_range": [0.0, 1.0]
        },
        {
            "default": "0.05",
            "name": "maximum q-value",
            "type": "double",
            "allowed_range": [0.0, 1.0]
        }
    ],
    "required_attributes": [
        "identifiers.entrez"
    ]
}
